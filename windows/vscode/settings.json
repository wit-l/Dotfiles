  "vim.foldfix": true,
  "vim.handleKeys": {
    "<C-f>": false,
    "<C-s>": false
  },
  "vim.highlightedyank.color": "rgba(230, 97, 90, 0.7)",
  "vim.highlightedyank.enable": true,
  "vim.highlightedyank.textColor": "white",
  "vim.hlsearch": true,
  "vim.easymotion": true,
  "vim.incsearch": true,
  "vim.insertModeKeyBindings": [
    {
      "after": ["<Esc>"],
      "before": ["j", "f"]
    }
  ],
  "vim.leader": "<space>",
  "vim.normalModeKeyBindingsNonRecursive": [
    {
      "before": ["z", "a"],
      "commands": ["editor.toggleFold"]
    },
    {
      "before": ["leader", "r", "n"],
      "commands": ["editor.action.rename"]
    },
    {
      "before": ["leader", "i"],
      "commands": ["extension.toggleBool"]
    },
    {
      "after": ["<C-w>", "j"],
      "before": ["<C-j>"]
    },
    {
      "after": ["<C-w>", "k"],
      "before": ["<C-k>"]
    },
    {
      "after": ["<C-w>", "h"],
      "before": ["<C-h>"]
    },
    {
      "after": ["<C-w>", "l"],
      "before": ["<C-l>"]
    },
    {
      "before": ["leader", "v"],
      "commands": [":vs"]
    },
    {
      "before": ["leader", "s"],
      "commands": [":sp"]
    },
    {
      "after": ["o", "<Esc>"],
      "before": ["o"]
    },
    {
      "after": ["O", "<Esc>"],
      "before": ["O"]
    },
    {
      "before": ["leader", "w"],
      "commands": [":w!"]
    },
    {
      "before": ["leader", "q"],
      "commands": [":q!"]
    },
    {
      "before": ["K"],
      "commands": ["lineBreakInsert"],
      "silent": true
    },
    {
      "before": ["leader", "k"],
      "commands": ["editor.action.showHover"]
    },
    {
      "before": ["]", "d"],
      "commands": ["editor.action.marker.next"]
    },
    {
      "before": ["[", "d"],
      "commands": ["editor.action.marker.prev"]
    },
    {
      "before": ["leader", "c", "a"],
      "commands": ["editor.action.quickFix"]
    },
    {
      "before": ["leader", "\\"],
      "commands": [":noh"]
    }
  ],
  "vim.useCtrlKeys": true,
  "vim.useSystemClipboard": true,
