{
  "$schema": "https://github.com/glzr-io/zebar/raw/v3.1.1/resources/zpack-schema.json",
  "name": "starter",
  "version": "0.0.0",
  "description": "",
  "tags": [],
  "previewImages": [],
  "repositoryUrl": "",
  "widgets": [
    {
      "name": "with-glazewm",
      "htmlPath": "./with-glazewm.html",
      "zOrder": "normal",
      "shownInTaskbar": false,
      "focused": true,
      "resizable": false,
      "transparent": true,
      "includeFiles": ["**/**"],
      "caching": {
        "defaultDuration": 604800,
        "rules": []
      },
      "privileges": {
        "shellCommands": []
      },
      "presets": [
        {
          "name": "default",
          "anchor": "top_center",
          "offsetX": "0px",
          "offsetY": "8px",
          "width": "100%",
          "height": "30px",
          "monitorSelection": {
            "type": "all"
          },
          "dockToEdge": {
            "enabled": false,
            "edge": "top",
            "windowMargin": "0px"
          }
        }
      ]
    }
  ]
}
